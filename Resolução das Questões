//2º

var someFN = function(numero) 
    {
        var valor = numero;

        return function (incremento)
        {
            valor += incremento;
            return valor;
        }
    }

    var counter  = someFN(1);

    console.log("primeiro call", counter(3));
    console.log("segundo call", counter(1));
    console.log("Terceiro call", counter(5));

